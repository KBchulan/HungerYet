}

/* 滚动条样式 */
QScrollBar:vertical {
    border: none;
    background: rgb(240, 240, 240);
    width: 8px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background: rgb(200, 200, 200);
    border-radius: 4px;
    min-height: 20px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
}

