/* 聊天消息容器 */
QWidget[isMessageContainer="true"] {
    background-color: transparent;
    margin: 2px 10px;
}

/* 消息文本样式 */
QLabel[messageLabel="true"] {
    padding: 12px 15px;
    border-radius: 15px;
    font-size: 14px;
    min-height: 20px;
}

/* 用户消息样式 */
QLabel[isUserMessage="true"] {
    background-color: rgb(255, 205, 97);
    color: rgb(0, 0, 0);
    margin-right: 5px;
    border: 1px solid rgb(245, 195, 87);
}

/* AI消息样式 */
QLabel[isUserMessage="false"] {
    background-color: rgb(255, 255, 255);
    color: rgb(0, 0, 0);
    margin-left: 5px;
    border: 1px solid rgb(220, 220, 220);
}

/* 聊天输入框样式 */
#inputEdit {
    border: 2px solid rgb(220, 220, 220);
    border-radius: 15px;
    padding: 10px 15px;
    background-color: white;
    min-height: 40px;
    max-height: 100px;
    font-size: 14px;
}

/* 发送按钮样式 */
#sendButton {
    background-color: rgb(255, 205, 97);
    border: 4px solid rgb(225, 170, 67);
    border-radius: 20px;
    color: rgb(0, 0, 0);
    padding: 5px 20px;
    min-width: 75px;
    min-height: 35px;
    font-size: 14px;
    font-weight: bold;
}

#sendButton:hover {
    background-color: rgb(255, 185, 77);
}

#sendButton:pressed {
    background-color: rgb(235, 185, 77);
}

/* 滚动区域样式 */
#scrollArea {
    border: none;
